{
  "name": "puppeteer-typescript-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate for Puppeteer + TypeScript",
  "repository": "https://github.com/sosmii/puppeteer-typescript-boilerplate",
  "author": "sosmii",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "tslint ./src/**/*.ts",
    "fix": "tslint --fix ./src/**/*.ts",
    "start": "ts-node src/scripts/index.ts",
    "start:headless": "ts-node src/scripts/index.ts true",
    "start:genetic": "ts-node src/scripts/startGenetic.ts",
    "starttest": "ts-node src/test.ts"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@types/node": "^10.14.13",
    "@types/puppeteer": "^1.12.4",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "puppeteer": "^1.18.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "async-genetic": "^1.1.1",
    "dotenv": "^8.2.0",
    "genetic": "^0.3.1",
    "lodash": "^4.17.20",
    "mockeer": "0.0.4"
  }
}
